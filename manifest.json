{
  "name": "Useful New Tab Page",
  "version": "1.1",
  "description": "A useful new tab replacement.",
  "permissions": [
    "bookmarks",
	"history",
	"topSites"
  ],
  "chrome_url_overrides" : {
    "newtab": "MyNewTab.html"
  },
  "icons" : {
		"16" : "newtb_16.png",
		"32" : "newtb_32.png",
		"48" : "newtb_48.png",
		"128" : "newtb_128.png"
	},
  "manifest_version": 2
}
