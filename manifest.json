{
  "name": "Useful New Tab Page",
  "version": "2.0",
  "description": "A useful new tab replacement.",
  "permissions": [
    "bookmarks",
	"history",
	"topSites",
    "contextMenus"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "chrome_url_overrides" : {
    "newtab": "MyNewTab.html"
  },
  "icons" : {
		"16" : "newtb_16.png",
		"32" : "newtb_32.png",
		"48" : "newtb_48.png",
		"128" : "newtb_128.png"
	},
  "manifest_version": 2
}
